{
    "manifest_version": 3,
    
    "name": "Internet Usage Tracker",
    "version": "1.0.1",
    "description": "Extension that tracks time spent on Chrome Browser",
    "author": "Kane Pereira",
    "browser_action":{
        "default_icon": "icon.jpg",
        "default_title": "Internet Usage Tracker",
        "default_popup": "index.html"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
        "activeTab",
        "tabs"
    ]
}